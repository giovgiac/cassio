data
    qword num;
    qword ind;

code
    function factorial(qword n) <- qword {
        if ([n] == 0) {
            return 1;
        };
        else {
            return [n] * factorial([n] - 1);
        };
    };

    function cassio() {
        [ind] <- 0;

        while ([ind] < 10) {
            [num] <- factorial([ind]);
            outputi([num]);
            [ind] <- [ind] + 1;
        };
    };
